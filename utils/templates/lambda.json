{
  "Type": "AWS::Lambda::Function",
  "Properties": {
    "Code": "../{{code}}",
    "Handler": "{{handler}}",
    "Role": {
      "Fn::GetAtt": [
        "RolLambda",
        "Arn"
      ]
    },
    "Runtime": "python3.11",
    "FunctionName": {
      "Fn::Sub": "${Environment}-${AppName}-{{name}}"
    },
    "MemorySize": {
      "Fn::If": [
        "isProd",
        1024,
        512
      ]
    },
    "Timeout": 30,
    "Environment": {
      "Variables": {
        "ENVIRONMENT": {
          "Ref": "Environment"
        },
        "APP_NAME": {
          "Ref": "AppName"
        },
        "LOG_LEVEL": "INFO",
        "POWERTOOLS_SERVICE_NAME": ""
      }
    },
    "TracingConfig": {
      "Mode": "Active"
    },
    "VpcConfig": {
      "SecurityGroupIds": [
        {
          "Ref": "SecurityGroup"
        }
      ],
      "SubnetIds": {
        "Fn::Split": [
          ",",
          {
            "Ref": "SubnetsIds"
          }
        ]
      }
    },
    "Layers": [
      {
        "Ref": "LayerCore"
      },
      {
        "Ref": "LayerPsycopg2"
      },
      {
        "Ref": "LayerLambdaPowertoolsCustom"
      },
      {
        "Ref": "LayerDbB2C"
      }
    ]
  }
}